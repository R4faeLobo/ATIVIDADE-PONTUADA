programa {
  funcao inicio() {
    //declarando variaveis
    inteiro a, b, soma, multiplicacao, resultado
  
   //atribuindo valores:
    escreva("Selecione o valor de A: ")
    leia(a)
    escreva("\nSelecione o valor de B: ")
    leia(b)

    se(a==b) 
    soma = a+b
    

    senao
    multiplicacao = a*b
    
  
    //exibindo
    escreva("Soma: ", soma)
    escreva("\nmultiplicação: ", multiplicacao)
    
  




  }
}
